CREATE pharamacies tables

CREATE TABLE pharmacies (id INT(11) AUTO_INCREMENT PRIMARY KEY, name VARCHAR(300), address VARCHAR(300), rate INT(5), drug VARCHAR(300), created_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP);
